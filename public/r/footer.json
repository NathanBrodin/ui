{
  "$schema": "https://ui.shadcn.com/schema/registry-item.json",
  "name": "footer",
  "type": "registry:ui",
  "dependencies": [
    "next/link"
  ],
  "registryDependencies": [
    "@brodin-ui/backgrounds",
    "@brodin-ui/diamond"
