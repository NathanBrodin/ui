{
  "$schema": "https://ui.shadcn.com/schema/registry-item.json",
  "name": "page-Header",
  "type": "registry:block",
  "description": "Page Heading",
  "registryDependencies": [],
  "files": [
    {
      "path": "registry/default/blocks/page-header/page-header.tsx",
      "content": "import Link from \"next/link\"\n\nimport { siteConfig } from \"@/lib/config\"\nimport { source } from \"@/lib/source\"\nimport { CommandMenu } from \"@/components/command-menu\"\nimport { GitHubLink } from \"@/components/github-link\"\nimport { Icons } from \"@/components/icons\"\nimport { MainNav } from \"@/components/main-nav\"\nimport { MobileNav } from \"@/components/mobile-nav\"\nimport { ModeSwitcher } from \"@/components/mode-switcher\"\nimport { SiteConfig } from \"@/components/site-config\"\nimport { Button } from \"@/registry/default/ui/button\"\nimport { Diamond } from \"@/registry/default/ui/diamond\"\nimport { Separator } from \"@/registry/default/ui/separator\"\n\nexport function PageHeader() {\n  const pageTree = source.pageTree\n\n  return (\n    <header className=\"border-grid bg-nav-background sticky top-0 z-50 w-full border-b\">\n      <div className=\"container-wrapper 3xl:fixed:px-0 px:3 md:px-8 lg:px-12\">\n        <div className=\"3xl:fixed:container border-grid flex h-(--header-height) items-center gap-2 border-x **:data-[slot=separator]:!h-4\">\n          <Diamond left bottom />\n          <Diamond right bottom />\n          <MobileNav\n            tree={pageTree}\n            items={siteConfig.navItems}\n            className=\"flex lg:hidden\"\n          />\n          <Button\n            asChild\n            variant=\"ghost\"\n            size=\"icon\"\n            className=\"hidden size-8 lg:flex\"\n          >\n            <Link href=\"/\">\n              <Icons.logo className=\"size-5\" />\n              <span className=\"sr-only\">{siteConfig.name}</span>\n            </Link>\n          </Button>\n          <MainNav items={siteConfig.navItems} className=\"hidden lg:flex\" />\n          <div className=\"ml-auto flex items-center gap-2 md:flex-1 md:justify-end\">\n            <div className=\"hidden w-full flex-1 md:flex md:w-auto md:flex-none\">\n              <CommandMenu tree={pageTree} />\n            </div>\n            <Separator\n              orientation=\"vertical\"\n              className=\"ml-2 hidden lg:block\"\n            />\n            <GitHubLink />\n            <Separator orientation=\"vertical\" className=\"3xl:flex hidden\" />\n            <SiteConfig className=\"3xl:flex hidden\" />\n            <Separator orientation=\"vertical\" />\n            <ModeSwitcher />\n          </div>\n        </div>\n      </div>\n    </header>\n  )\n}\n",
      "type": "registry:block"
    }
  ],
  "categories": [
    "page-layout"
  ]
}